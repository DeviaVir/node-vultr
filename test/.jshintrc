{
  "node": true,
  "browser": false,
  "maxcomplexity": 15,
  "curly": true,
  "laxcomma": true,
  "laxbreak": true,
  "strict": false,
  "quotmark": false,
  "undef": true,
  "unused": "vars",
  "sub": true,
  "expr": true,
  "-W043": true,
  "predef": [
    "-Promise",
    "describe",
    "it",
    "before",
    "after",
    "beforeEach",
    "afterEach",
    "expect"
  ]
}
